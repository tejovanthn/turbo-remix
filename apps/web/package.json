{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "clean": "rm -rf .turbo .build node_modules",
    "dev": "run-p dev:*",
    "dev:arc": "node ./dev sandbox",
    "dev:remix": "remix watch",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:4200 \"npx cypress open\"",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test dev http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc && tsc -p cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/server/index.js",
    "/public/build"
  ],
  "dependencies": {
    "@architect/architect": "^10.8.4",
    "@architect/functions": "^5.3.3",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@mantine/core": "^5.10.3",
    "@mantine/hooks": "^5.10.3",
    "@mantine/remix": "^5.10.3",
    "@remix-run/architect": "^1.12.0",
    "@remix-run/node": "^1.12.0",
    "@remix-run/react": "^1.12.0",
    "@remix-run/server-runtime": "^1.12.0",
    "@tabler/icons": "^2.2.0",
    "bcryptjs": "2.4.3",
    "cuid": "^3.0.0",
    "isbot": "^3.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@remix-run/dev": "^1.12.0",
    "@remix-run/eslint-config": "^1.12.0",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/architect__functions": "^3.13.7",
    "@types/bcryptjs": "2.4.2",
    "@types/eslint": "^8.4.10",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.0.1",
    "@vitest/coverage-c8": "^0.28.4",
    "c8": "^7.12.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.5.1",
    "esbuild": "^0.17.2",
    "eslint": "^8.32.0",
    "eslint-config-remix": "*",
    "happy-dom": "^8.2.6",
    "mock-aws-s3": "^4.0.2",
    "msw": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "start-server-and-test": "^1.15.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.28.4"
  },
  "engines": {
    "node": ">=14"
  }
}
